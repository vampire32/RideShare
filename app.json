{
  "expo": {
    "name": "RideShare",
    "slug": "cabApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logoicon.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "splash": {
      "image": "./assets/RideShareSplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2532fc"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.moiz210.cabApp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.moiz210.cabApp",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "49c701d1-3ff6-4915-8d4f-91ccfcaf75d2"
      }
    }
  }
}
